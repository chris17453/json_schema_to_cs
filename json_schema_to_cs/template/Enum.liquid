{% if HasDescription -%}
/// <summary>{{ Description | csharpdocs }}</summary>
{% endif -%}
[DataContract(Name = "{{ Name }}")]
[XmlType(Namespace = "http://{{Namespace}}.xsd")] 
{{ TypeAccessModifier }} enum {{ Name }}
{
{% for enum in Enums -%}
{%   if IsStringEnum -%}
    [EnumMember(Value = "{{ enum.Value }}")]
{%   endif -%}
    {{ enum.Name }} = {{ enum.InternalValue }},

{% endfor -%}
}